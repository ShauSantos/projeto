    public TelaInfo2() {
        initComponents();
        int ctrl = TelaAlterar.getCtrl();
        if(ctrl==1){
            jLabel11.setText("Informações atualizadas");
        }
        i=TelaInfo.getI();
        nome = TelaCadastro.getNome(i);
        cpf = TelaCadastro.getCPF(i);
        email = TelaCadastro.getEMail(i);
        tel = TelaCadastro.getTel(i);
        data = TelaCadastro.getData(i);
        
        jLabel5.setText(nome);
        jLabel6.setText(cpf);
        jLabel8.setText(data);
        jLabel10.setText(email);
        jLabel9.setText(tel);
    }

